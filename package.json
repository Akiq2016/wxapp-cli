{
  "name": "@hzfe/wxapp-cli",
  "version": "0.1.2",
  "description": "自用小程序脚手架",
  "main": "lib/index.js",
  "scripts": {
    "format": "prettier --trailing-comma es5 --single-quote --write 'src/*/*.js'",
    "test": "cross-env ENV=development jest",
    "dev": "cross-env ENV=development ./bin/wxa",
    "prebuild": "rimraf lib && babel src -d lib && cpy '**/template.*' '../lib/' --cwd=src --parents",
    "build": "cross-env jest",
    "release": "standard-version"
  },
  "bin": {
    "wxa": "bin/wxa"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feiwuteam/wxapp-cli.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/feiwuteam/wxapp-cli/issues"
  },
  "homepage": "https://github.com/feiwuteam/wxapp-cli#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "@commitlint/cli": "^7.2.0",
    "@commitlint/config-conventional": "^7.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "cpy-cli": "^2.0.0",
    "cross-env": "^5.2.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "sao": "^0.22.17",
    "standard-version": "^4.4.0"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "fs-extra": "^7.0.0",
    "inquirer": "^6.2.0",
    "mkdirp": "^0.5.1",
    "xdg-basedir": "^3.0.0",
    "yargs": "^12.0.2"
  },
  "lint-staged": {
    "*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  }
